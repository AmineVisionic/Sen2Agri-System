<operator>
  <name>org.esa.sen2agri.vegetationphenoprocessor</name>
  <operatorClass>org.esa.snap.core.gpf.operators.tooladapter.ToolAdapterOp</operatorClass>
  <alias>VegetationPhenoProcessor</alias>
  <label>Vegetation Phenology Processor</label>
  <version>1.0</version>
  <description>Sen2Agri Vegetation Phenology Processor</description>
  <authors>Ramona Manda</authors>
  <copyright>(C)2015 CS ROMANIA</copyright>
  <internal>false</internal>
  <autoWriteSuppressed>false</autoWriteSuppressed>
  <menuLocation>Menu/Tools/External Tools</menuLocation>
  <preprocessTool>false</preprocessTool>
  <writeForProcessing>false</writeForProcessing>
  <mainToolFileLocation>$PROCESSORS_PATH/VegetationStatus/TestScripts/pheno_processing.py</mainToolFileLocation>
  <workingDir>$WORKING_DIR_PATH/Vegetation</workingDir>
  <templateFileLocation>VegetationPhenoProcessor-template.vm</templateFileLocation>
  <progressPattern>(?:.+): (\d{1,3})% (?:.+)</progressPattern>
  <errorPattern>ERROR: (.+)</errorPattern>
  <variables>
    <osvariable>
      <key>PYTHON_BIN</key>
      <value></value>
      <windows>python.exe</windows>
      <linux></linux>
      <macosx>/bin/python2.7</macosx>
      <isTransient>false</isTransient>
    </osvariable>
    <variable>
      <key>WORKING_DIR_PATH</key>
      <value>/home/ramonag/sen2agri/workingDir/</value>
    </variable>
    <variable>
      <key>PROCESSORS_PATH</key>
      <value></value>
    </variable>
    <variable>
      <key>SCRIPT_PATH</key>
      <value>/home/ramonag/sen2agri/sources/sen2agri/</value>
    </variable>
    <variable>
      <key>SENTINEL_BUILD_PATH</key>
      <value>/home/ramonag/sen2agri/sources/sen2agri-build/</value>
    </variable>
  </variables>
  <parameters>
    <parameter>
      <name>targetProductFile</name>
      <dataType>java.io.File</dataType>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
      <toolParameterDescriptors/>
    </parameter>
    <parameter>
      <name>inputFiles</name>
      <dataType>[Ljava.io.File;</dataType>
      <description>the list of files representing the metadata files of the products</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
      <toolParameterDescriptors/>
    </parameter>
    <parameter>
      <name>startDate</name>
      <dataType>java.lang.String</dataType>
      <defaultValue>20130206</defaultValue>
      <description>The start date for the temporal resampling interval (in format YYYYMMDD)</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
      <toolParameterDescriptors/>
    </parameter>
    <parameter>
      <name>endDate</name>
      <dataType>java.lang.String</dataType>
      <defaultValue>20130616</defaultValue>
      <description>The enddate for the temporal resampling interval (in format YYYYMMDD)</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
      <toolParameterDescriptors/>
    </parameter>
    <parameter>
      <name>tileId</name>
      <dataType>java.lang.String</dataType>
      <defaultValue>T0000</defaultValue>
      <description>Tile id</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
      <toolParameterDescriptors/>
    </parameter>
    <parameter>
      <name>resolution</name>
      <dataType>java.lang.Integer</dataType>
      <description>Resample to this resolution. Use the same resolution as input files if you don&apos;t want any resample</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
      <toolParameterDescriptors/>
    </parameter>
  </parameters>
  <source>package</source>
  <isSystem>false</isSystem>
  <isHandlingOutputName>true</isHandlingOutputName>
  <sourceProductDescriptors/>
  <targetPropertyDescriptors/>
